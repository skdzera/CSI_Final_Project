{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "csiadfsk"
        },
        "AzureSqlDatabase1_password": {
            "type": "secureString",
            "metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
        },
        "SqlServer2_password": {
            "type": "secureString",
            "metadata": "Secure string for 'password' of 'SqlServer2'"
        },
        "csiadlslinked1_accountKey": {
            "type": "secureString",
            "metadata": "Secure string for 'accountKey' of 'csiadlslinked1'"
        },
        "AzureSqlDatabase1_properties_typeProperties_server": {
            "type": "string",
            "defaultValue": "csiproject.database.windows.net"
        },
        "AzureSqlDatabase1_properties_typeProperties_database": {
            "type": "string",
            "defaultValue": "ssdb"
        },
        "AzureSqlDatabase1_properties_typeProperties_userName": {
            "type": "string",
            "defaultValue": "skdzera"
        },
        "SqlServer2_properties_typeProperties_server": {
            "type": "string",
            "defaultValue": "SKDZERA\\SQLEXPRESS"
        },
        "SqlServer2_properties_typeProperties_database": {
            "type": "string",
            "defaultValue": "sakshamdb"
        },
        "SqlServer2_properties_typeProperties_userName": {
            "type": "string",
            "defaultValue": "SKDZERA\\Saksham"
        },
        "csiadlslinked1_properties_typeProperties_url": {
            "type": "string",
            "defaultValue": "https://strgsk.dfs.core.windows.net/"
        },
        "integrationRuntime2_properties_typeProperties_linkedInfo_resourceId": {
            "type": "string",
            "defaultValue": "/subscriptions/3df2ff29-09fa-4393-b872-1121e7b6a3ce/resourcegroups/csi_sk/providers/Microsoft.DataFactory/factories/skadf096/integrationruntimes/IR-CSI6"
        },
        "containerUri": {
            "type": "string",
            "metadata": "URI to storage account containing linked ARM templates"
        },
        "containerSasToken": {
            "type": "string",
            "metadata": "Sas token to storage account containing linked ARM templates"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "csiadfsk_ArmTemplate_0",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2024-03-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "factoryName": {
                        "value": "[parameters('factoryName')]"
                    },
                    "csiadlslinked1_accountKey": {
                        "value": "[parameters('csiadlslinked1_accountKey')]"
                    },
                    "SqlServer2_password": {
                        "value": "[parameters('SqlServer2_password')]"
                    },
                    "AzureSqlDatabase1_password": {
                        "value": "[parameters('AzureSqlDatabase1_password')]"
                    },
                    "csiadlslinked1_properties_typeProperties_url": {
                        "value": "[parameters('csiadlslinked1_properties_typeProperties_url')]"
                    },
                    "integrationRuntime2_properties_typeProperties_linkedInfo_resourceId": {
                        "value": "[parameters('integrationRuntime2_properties_typeProperties_linkedInfo_resourceId')]"
                    },
                    "SqlServer2_properties_typeProperties_server": {
                        "value": "[parameters('SqlServer2_properties_typeProperties_server')]"
                    },
                    "SqlServer2_properties_typeProperties_database": {
                        "value": "[parameters('SqlServer2_properties_typeProperties_database')]"
                    },
                    "SqlServer2_properties_typeProperties_userName": {
                        "value": "[parameters('SqlServer2_properties_typeProperties_userName')]"
                    },
                    "AzureSqlDatabase1_properties_typeProperties_server": {
                        "value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
                    },
                    "AzureSqlDatabase1_properties_typeProperties_database": {
                        "value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
                    },
                    "AzureSqlDatabase1_properties_typeProperties_userName": {
                        "value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
                    }
                }
            },
            "dependsOn": []
        }
    ]
}